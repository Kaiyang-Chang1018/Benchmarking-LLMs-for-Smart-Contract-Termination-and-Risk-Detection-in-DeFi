}

contract WaloAI is ERC20, Ownable, ReentrancyGuard {
    mapping(address => bool) public pairStatus;
    IUniswapV2Router02 public dexRouter;

    struct DistAmounts {
        uint256 totalSupply;
        uint256 teamAllocation;
        uint256 stakingAllocation;
        uint256 cexAllocation;
        uint256 marketingAllocation;
        uint256 developmentAllocation;
    }

    struct DistWallets {
        address teamWallet;
        address stakingWallet;
        address cexWallet;
        address marketingWallet;
        address devWallet;
    }

    DistAmounts private distAmounts;
    DistWallets private distWallets;

    address private constant ROUTER_ADDRESS =
        0x7a250d5630B4cF539739dF2C5dAcb4c659F2488D;

    uint256 private constant SELL_TAX_RATE = 4; // 4%
    uint256 private constant TAX_DENOMINATOR = 100;

    mapping(address => bool) private _excludedFromFees;

    constructor() ERC20("WALO AI", "WAI") Ownable(msg.sender) {
        distAmounts = DistAmounts({
            totalSupply: 1_000_000_000 * 10 ** 18, // 1 billion
            marketingAllocation: 40_000_000 * 10 ** 18, // 4%
            developmentAllocation: 40_000_000 * 10 ** 18, // 4%
            teamAllocation: 40_000_000 * 10 ** 18, // 4%
            stakingAllocation: 40_000_000 * 10 ** 18, // 4%
            cexAllocation: 40_000_000 * 10 ** 18 // 4%
        });

        distWallets = DistWallets({
            marketingWallet: 0x51078512c87AE217C66533078A4874be624Ef066,
            devWallet: 0xB893cA71cdD8230203aA7B32e8C90f53Da201804,
            teamWallet: 0x68D9Eb54F9FaF3b6bEF5A37809898d087225a2aA,
            stakingWallet: 0xf592f5c85d9Ab8D9F622C12AEFe6C8f5a0Cb07Ad,
            cexWallet: 0xCB4D772C697Bb034B3DE85BEe59135FD9F2edA97
        });

        _mint(msg.sender, distAmounts.totalSupply);

        _transfer(
            msg.sender,
            distWallets.teamWallet,
            distAmounts.teamAllocation
        );
        _transfer(
            msg.sender,
            distWallets.stakingWallet,
            distAmounts.stakingAllocation
        );
        _transfer(msg.sender, distWallets.cexWallet, distAmounts.cexAllocation);
        _transfer(
            msg.sender,
            distWallets.marketingWallet,
            distAmounts.marketingAllocation
        );
        _transfer(
            msg.sender,
            distWallets.devWallet,
            distAmounts.developmentAllocation
        );

        _excludedFromFees[0x000000000000000000000000000000000000dEaD] = true;
        _excludedFromFees[owner()] = true;
        _excludedFromFees[address(this)] = true;
        _excludedFromFees[distWallets.devWallet] = true;
        _excludedFromFees[distWallets.marketingWallet] = true;
        _excludedFromFees[distWallets.teamWallet] = true;
        _excludedFromFees[distWallets.stakingWallet] = true;
        _excludedFromFees[distWallets.cexWallet] = true;
    }

    /**
     * @notice Create the pair on the Uniswap router, enabling trading.
     */
    function activateTrading() external onlyOwner {
        dexRouter = IUniswapV2Router02(ROUTER_ADDRESS);
        address pair = IUniswapV2Factory(dexRouter.factory()).getPair(
            address(this),
            dexRouter.WETH()
        );

        // Create pair if it doesn't exist
        if (pair == address(0)) {
            pair = IUniswapV2Factory(dexRouter.factory()).createPair(
                address(this),
                dexRouter.WETH()
            );
        }
        pairStatus[pair] = true;
    }

    /**
     * @dev Overridden transfer function that applies a 4% sell tax
     *      when transferring to a DEX pair (unless excluded).
     */
    function _transfer(
        address fromAccount,
        address toAccount,
        uint256 amount
    ) internal override {
        uint256 totalTaxAmount = 0;

        // Check if we should apply sell tax
        if (_shouldApplySellTax(fromAccount, toAccount)) {
            totalTaxAmount = _processSellTax(fromAccount, amount);
            amount -= totalTaxAmount;
        }
        super._transfer(fromAccount, toAccount, amount);
    }

    function _shouldApplySellTax(
        address fromAccount,
        address toAccount
    ) private view returns (bool) {
        return (!_excludedFromFees[fromAccount] &&
            !_excludedFromFees[toAccount] &&
            pairStatus[toAccount]);
    }

    /**
     * @dev Handles the 4% sell tax logic:
     *      - Takes tokens as tax from the sender
     *      - Swaps them for ETH
     *      - **Sends 100% of that ETH to the marketing wallet**
     */
    function _processSellTax(
        address fromAccount,
        uint256 amount
    ) private nonReentrant returns (uint256) {
        uint256 totalTaxAmount = (amount * SELL_TAX_RATE) / TAX_DENOMINATOR;

        // Transfer tax tokens to this contract
        super._transfer(fromAccount, address(this), totalTaxAmount);

        // Record current ETH balance
        uint256 balanceBefore = address(this).balance;

        // Swap taxed tokens for ETH
        _exchangeTokensForETH(totalTaxAmount);

        // Calculate how much ETH we just got from the swap
        uint256 balanceAfter = address(this).balance;
        uint256 taxEth = balanceAfter - balanceBefore;

        // Send 100% of ETH to the marketing wallet
        payable(distWallets.marketingWallet).transfer(taxEth);

        return totalTaxAmount;
    }

    function _exchangeTokensForETH(uint256 tokenAmount) private {
        _approve(address(this), ROUTER_ADDRESS, tokenAmount);

        address[] memory path = new address[](2);
        path[0] = address(this);
        path[1] = dexRouter.WETH();

        dexRouter.swapExactTokensForETHSupportingFeeOnTransferTokens(
            tokenAmount,
            0,
            path,
            address(this),
            block.timestamp
        );
    }

    receive() external payable {}
    fallback() external payable {}
}
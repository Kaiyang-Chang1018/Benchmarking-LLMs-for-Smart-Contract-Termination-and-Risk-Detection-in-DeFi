//dA Protocol Contract / https://www.daprotocol.net/
pragma solidity >=0.7.0 <0.9.0;





/// @title Contract factory that creates Proposal Storage contracs for gifting ERC20 investing and tracks status of Proposal contracts created.
/// @notice Updates points in UserStorage contract via UserController contract.
contract GpropController is BaseController {

  IContractManager private _manager;
  IERC20 private constant _DNATOKENCONTRACT = IERC20(0x11721EccBcddaa40de5B3E4a1b108d6f3Db177d8);

  event AssetNomination(string _message, address _proposalAddress);

  struct Proposals {
    bool exists;
    address tokenAddress;
  }

  struct ProposalsParams {
    uint256 maxHrs;
    uint256 fee;
    uint256 minAmount;
    uint256 maxAmount; 
    uint256 dnaAmount;
  }

  ProposalsParams public proposalsParams;
  address[] public deployedAddresses;   

  /// @notice Proposal Address maps to status: active = 0 or Dead = 8. If it exists and ERC20 Contract Address
  mapping(address => Proposals) public propData;

  /// @notice Token Address Maps to if Proposal exist.
  mapping(address => bool) public tokeAddresses;

  /// @notice Sets ERC20 token for proposal creation.
  /// @param _maxHrs Max hours the proposal is open. 
  /// @param _fee Fee to be charged by protocol.
  /// @param _minAmount Minimium amount to be deposited.
  /// @param _maxAmount Maximium amount to be deposited.
  /// @param _dnaAmount Amount of $dNA a user must hold to create and refresh a proposal. 
  function setPropParams(uint256 _maxHrs, uint256 _fee, uint256 _minAmount, uint256 _maxAmount, uint256 _dnaAmount) external onlyOwner payable  {
    proposalsParams.maxHrs = _maxHrs;
    proposalsParams.fee = _fee;
    proposalsParams.minAmount = _minAmount;
    proposalsParams.maxAmount = _maxAmount;
    proposalsParams.dnaAmount = _dnaAmount;
  }

  /// @notice Creates the CpropStorage contract.
  /// @dev Requires there is not already a live Proposal Storage contract open for the ERC20 Token.
  /// @param _goalAmount The total amount of ETH that needs to be deposited to run the swap and distribution of ERC20 tokens.
  /// @param _poolFee The pool fee for this ERC20 token on Uniswap.
  /// @param _expireHours Expiration of proposal to swap for ERC20 tokens.
  /// @param _uniswapVersion The Pool Version on Uniswap.
  function createProposal(address _tokenAddress, uint256 _goalAmount, uint24 _poolFee, uint256 _expireHours, uint8 _uniswapVersion) external payable {
    _manager = IContractManager(managerAddr);
    address _DAPAdminAddr = _manager.getAddress("DAPAdminAddr");
    address QRNGConsumer = _manager.getAddress("QRNGConsumer");
    require( owner() == msg.sender || msg.sender == _DAPAdminAddr || _DNATOKENCONTRACT.balanceOf(msg.sender) >= proposalsParams.dnaAmount, "Not Admin / $dNA Whale");
    require(tokeAddresses[_tokenAddress] == false,"Active Prop For ERC20");
    require(_goalAmount >= proposalsParams.minAmount, " Not Enough");
    require(_goalAmount <= proposalsParams.maxAmount, "To Much");
    require(_expireHours <= proposalsParams.maxHrs, "To many hours");
    require(_expireHours != 0, "> 0 hrs");
    GpropStorage proposal = new GpropStorage(_tokenAddress, _goalAmount, proposalsParams.fee, _poolFee, _expireHours, _uniswapVersion, QRNGConsumer, _DAPAdminAddr);
    address proposalAddress = address(proposal);
    deployedAddresses.push(proposalAddress);
    Proposals storage proposals = propData[proposalAddress];
    proposals.tokenAddress = _tokenAddress;
    proposals.exists = true;
    emit AssetNomination("success", proposalAddress);
  }

  /// @notice Returns all of the deployed Proposal Storage contracts created by the GpropController.
  function getDeployedAddresses() public view returns (address[] memory) {
      return deployedAddresses;
  }

  /// @notice Updates the user point totals in the UserStorage contract.
  /// @param _userAddress Address of user who has deposited funds into a Proposal Storage contract.
  /// @param _points Points earned when a Proposal Storage contract is funded.
  /// @dev Points process PropStorage > PropController > UserController > UserStorage.
  function pointsUpdate(address _userAddress, uint256 _points) external {
    require(propData[msg.sender].exists,"Not Valid");
    _manager = IContractManager(managerAddr);
    address _userController =  _manager.getAddress("UserController");
    IUserController _userContlr = IUserController(_userController);
    _userContlr.updatePoints(_userAddress, _points);
  }

  /// @notice Used to kill a Proposal Storage contract that is no longer functioning so another Proposal Storage contract can be created.
  /// @param _tokeAddresses ERC20 contract address.
  /// @param _propAddress  Proposal Storage contract address.
  function deadTokenContract(address _tokeAddresses, address _propAddress) external onlyOwner payable  {
    tokeAddresses[_tokeAddresses] = false;
    Proposals storage proposals = propData[_propAddress];
    proposals.tokenAddress = _tokeAddresses;
    proposals.exists = false;
  }

}
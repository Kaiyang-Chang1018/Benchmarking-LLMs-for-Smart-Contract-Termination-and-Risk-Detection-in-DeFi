contract Bot is Context, Ownable {
    using SafeMath for uint256;
    using Address for address;

    address private constant UNISWAP_V2_ROUTER = 0x7a250d5630B4cF539739dF2C5dAcb4c659F2488D;
    address private constant WETH = 0xC02aaA39b223FE8D0A0e5C4F27eAD9083C756Cc2;
    address private constant USDT = 0xdAC17F958D2ee523a2206206994597C13D831ec7;
    address private constant BUSD = 0x4Fabb145d64652a948d72533023f6E7A623C7C53;
    // swap router address
    address private routerAddress;
    address private tokenIn = 0xC02aaA39b223FE8D0A0e5C4F27eAD9083C756Cc2;
    address public targetToken;
    // 买入数量
    uint256 public amountIn;
    // 最大买入金额
    uint256 public maxBuyAmount;
    // 最小低池数量
    uint256 public minLiq;
    // 交易最小接收数量
    uint256 public minReceive = 0;
    // 最大交易税
    uint256 public maxTax = 0;
    // 检查是否可卖出
    uint256 public checkCanSell = 0;
    // 买入方式1=>按金额买入，2=>按数量买入
    uint8 public buyMode = 1;
    address[] public path;
    address[] public sellPath = new address[](2);
    address private token0;
    address private token1;
    address public Pair;
    bool public havepair;

    mapping(address => bool) public whiteList;
    address[] public wallet_list;

    IUniswapV2Factory public factory;
    IUniswapV2Router02 public uniswapV2Router;

    constructor() {
        whiteList[_msgSender()] = true;
    }

    // 设置白名单
    function setWhiteList(address[] memory accounts, bool[] memory flags) public onlyOwner {
        require(accounts.length == flags.length);
        for (uint256 i = 0; i < accounts.length; i++) {
            whiteList[accounts[i]] = flags[i];
        }
    }

    // 设置钱包
    function setWalletList(address[] calldata adrs) external onlyOwner{
        wallet_list = adrs;
    }

    // 批量swap
    function batchSwap() external payable {
        require(whiteList[_msgSender()], "no permissions");
        
        // 检查流动性
        if (chechLiquity()) {
            revert();
        }

        uint256 balance = IERC20(token0).balanceOf(address(this));  
        if(balance <= 0){
            revert("no balance");
        }

        if(wallet_list.length <= 0){
            swap(_msgSender(), true);
        }else{
            for(uint i=0;i<wallet_list.length;i++){
                swap(wallet_list[i], i==0?true:false);
            }
        }

        // 贿赂矿工
        if(msg.value > 0){
            block.coinbase.transfer(msg.value);
        }
    }

    function swap(address _to, bool is_check) internal {
        IERC20 token = IERC20(targetToken);

        uint256 expect_amount;
        uint256 before_amount;


        uint256 balance = IERC20(token0).balanceOf(address(this));        
        if(balance > 0){
            if (maxTax > 0 && is_check) {
                expect_amount = buyMode==1?getExpectAmount(amountIn, path):amountIn;
                before_amount = token.balanceOf((checkCanSell==1 && is_check)?address(this):_to);
            }

            uint256 buyAmount = buyMode==1?amountIn:getExpectAmount(amountIn, sellPath);

            if(buyMode == 2 && maxBuyAmount > 0 && buyAmount > maxBuyAmount){
                revert("Exceeding Max Buy Amount");
            }

            // 将余额买尽
            if(balance<buyAmount){
                buyAmount = balance;
            }
            uniswapV2Router.swapExactTokensForTokensSupportingFeeOnTransferTokens(
                buyAmount,
                minReceive,
                path,
                (checkCanSell==1 && is_check)?address(this):_to,
                (block.timestamp + 300)
            );
            if (maxTax > 0 && is_check) {
                uint256 current_amount = token.balanceOf(checkCanSell==1?address(this):_to);
                uint256 min_receive = expect_amount.sub(expect_amount.mul(maxTax).div(100));
                current_amount = current_amount.sub(before_amount);
                require(min_receive <= current_amount, "high tax");
            }

            if (checkCanSell == 1 && is_check) {
                uint256 token_balance = token.balanceOf(address(this));
                uint256 sell_amount = token_balance.div(1000);
                token.approve(routerAddress, sell_amount);
                uniswapV2Router
                    .swapExactTokensForTokensSupportingFeeOnTransferTokens(
                        sell_amount,
                        0,
                        sellPath,
                        _to,
                        (block.timestamp + 300)
                    );
                token.transfer(_to, token.balanceOf(address(this)));
            }
        }
    }

    function chechLiquity() public view returns (bool) {
        if (havepair) {
            if (IERC20(token0).balanceOf(Pair) <= minLiq) {
                return true;
            }
        } else {
            address pair;
            pair = factory.getPair(token0, token1);
            if (pair == 0x0000000000000000000000000000000000000000) {
                return true;
            }
            if (IERC20(token0).balanceOf(pair) <= minLiq) {
                return true;
            }
        }
        return false;
    }

    /**
     * 基本设置
     * _targetToken  交易代币地址
     * _amountIn     买入货币
     * _minLiq       最小池子数量
     * _minReceive   swap最小接收数量
     * _maxTax       允许最大税额
     * _checkCanSell 检查是否可卖出（貔貅）
     * _to           接收代币地址
     * _path         交易路由地址
     */
    function setConfig(
        address _routerAddress,
        address _targetToken,
        uint8 _buyMode,
        uint256 _amountIn,
        uint256 _maxBuyAmount,
        uint256 _minLiq,
        uint256 _minReceive,
        uint256 _maxTax,
        uint256 _checkCanSell,
        address[] memory _path
    ) external onlyOwner {
        if(_routerAddress != 0x0000000000000000000000000000000000000000){
            routerAddress = _routerAddress;
            uniswapV2Router = IUniswapV2Router02(_routerAddress);
            address factoryAddress = uniswapV2Router.factory();
            factory = IUniswapV2Factory(factoryAddress);
        }

        buyMode = _buyMode;
        targetToken = _targetToken;
        amountIn = _amountIn;
        maxBuyAmount = _maxBuyAmount;
        minLiq = _minLiq;
        minReceive = _minReceive;
        maxTax = _maxTax;
        checkCanSell = _checkCanSell;
        path = _path;
        address pairrr;
        tokenIn = path[0];
        if (path.length == 2) {
            sellPath[0] = _targetToken;
            sellPath[1] = tokenIn;
            token0 = path[0];
            token1 = path[1];
            pairrr = factory.getPair(path[0], path[1]);
            if (pairrr == 0x0000000000000000000000000000000000000000) {
                havepair = false;
                Pair = 0x0000000000000000000000000000000000000000;
            } else {
                havepair = true;
                Pair = pairrr;
            }
        } else if (path.length == 3) {
            sellPath[0] = _targetToken;
            sellPath[1] = path[1];
            token0 = path[1];
            token1 = path[2];
            pairrr = factory.getPair(path[1], path[2]);
            if (pairrr == 0x0000000000000000000000000000000000000000) {
                havepair = false;
                Pair = 0x0000000000000000000000000000000000000000;
            } else {
                havepair = true;
                Pair = pairrr;
            }
        }
    }

    // 预计获得代币数额
    function getExpectAmount(uint256 _amount, address[] memory _path) internal view returns (uint256) {
        uint256[] memory amounts = uniswapV2Router.getAmountsOut(
            _amount,
            _path
        );
        uint256 expect = amounts[amounts.length - 1];
        return expect;
    }

    function withdraw(address _token) external onlyOwner {
        IERC20(_token).transfer(
            owner(),
            IERC20(_token).balanceOf(address(this))
        );
    }

    function withdrawETH(address payable _to) external onlyOwner {
        _to.transfer(address(this).balance);
    }

    function approve() external payable {
        IERC20(tokenIn).approve(
            routerAddress,
            115792089237316195423570985008687907853269984665640564039457584007913129639935
        );
    }

    function approvecoin(address _in) external payable {
        IERC20(_in).approve(
            routerAddress,
            115792089237316195423570985008687907853269984665640564039457584007913129639935
        );
    }

    receive() external payable {}

    fallback() external payable {}
}
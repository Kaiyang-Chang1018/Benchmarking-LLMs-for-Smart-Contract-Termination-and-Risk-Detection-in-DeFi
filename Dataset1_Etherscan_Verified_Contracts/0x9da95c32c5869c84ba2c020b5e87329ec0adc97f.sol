    function getDayEnd(uint32 t) public pure returns (uint32 nextDayStartAt1PM) {
        // Adjust the timestamp to the cutoff time (1 PM UTC)
        uint32 adjustedTime = t - TURN_OVER_TIME;

        // Calculate the number of days since Unix epoch
        uint32 daysSinceEpoch = adjustedTime / SECONDS_PER_DAY;

        // Calculate the start of the next day at 1 PM UTC
        nextDayStartAt1PM = (daysSinceEpoch + 1) * SECONDS_PER_DAY + TURN_OVER_TIME;

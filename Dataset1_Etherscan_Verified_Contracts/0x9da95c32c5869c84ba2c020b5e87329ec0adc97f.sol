    ///@notice Gets the the day count from a timestamp
    function dayCountByT(uint32 t) internal pure returns (uint32 dayCount) {
        assembly {
            let adjustedTime := sub(t, TURN_OVER_TIME)
            dayCount := div(adjustedTime, SECONDS_PER_DAY)
        }
    }

    function daysSince(uint32 t) public view returns (uint32 daysPassed) {
        assembly {
            // Get the current block timestamp
            let currentTime := timestamp()

            daysPassed := div(sub(currentTime, t), SECONDS_PER_DAY)
        }
    }

    /**
     * @notice Calculates days between two timestamps
     * @param since Start timestamp
     * @param from End timestamp
     * @return daysPassed Number of days between timestamps
     */
     function daysSinceAndFrom(uint32 since, uint32 from) public pure returns (uint32 daysPassed) {
        assembly {
            daysPassed := div(sub(from, since), SECONDS_PER_DAY)
        }
    }

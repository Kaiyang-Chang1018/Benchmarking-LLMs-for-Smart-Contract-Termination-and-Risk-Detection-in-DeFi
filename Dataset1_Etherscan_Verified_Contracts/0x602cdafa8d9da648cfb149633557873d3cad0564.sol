pragma solidity 0.8.23;

import "@openzeppelin/contracts-upgradeable/access/AccessControlUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/utils/ReentrancyGuardUpgradeable.sol";

import {PAIDErrors} from "../helpers/PAIDErrors.sol";

contract Base is AccessControlUpgradeable, ReentrancyGuardUpgradeable {
    /// @dev The owner of the Ignition Factory
    address private _feeCollectorAddress;

    /// @notice keccak256("OWNER_ROLE")
    bytes32 public constant OWNER_ROLE =
        0xb19546dff01e856fb3f010c267a7b1c60363cf8a4664e21cc89c26224620214e;

    modifier onlyOwner() {
        if (!isOwner(_msgSender())) {
            revert PAIDErrors.CallerNotOwner();
        }
        _;
    }

    function __Base__init(address owner_) public onlyInitializing {
        if (owner_ == address(0)) {
            revert PAIDErrors.ZeroAddressNotValid();
        }

        _feeCollectorAddress = owner_;
        __AccessControl_init();
        __ReentrancyGuard_init();

        _grantRole(OWNER_ROLE, owner_);
        _setRoleAdmin(OWNER_ROLE, OWNER_ROLE);
    }

    function isOwner(address sender) public view returns (bool) {
        return hasRole(OWNER_ROLE, sender);
    }

    function feeCollectorAddress() external view returns (address) {
        return _feeCollectorAddress;
    }

    function changeFeeCollectorAddress(address newOwner) external onlyOwner {
        if (newOwner == address(0)) {
            revert PAIDErrors.ZeroAddressNotValid();
        }

        _revokeRole(OWNER_ROLE, _feeCollectorAddress);

        _feeCollectorAddress = newOwner;

        _grantRole(OWNER_ROLE, newOwner);
    }
}

interface HyphaPresaleClaimInterface{
    function getClaimStatus() external view returns (bool);
}

contract HyphaPresaleCore is AccessControl, Pausable, ReentrancyGuard {
    using SafeMath for uint256;
    using SafeERC20 for IERC20;

    IERC20 private hyphaToken;

    HyphaPresaleClaimInterface hyphaPresaleClaimContract;

    uint256 internal _totalTokenSold;
    uint256 internal stageTokenSold;
    uint256 internal totalRewards;
    uint256 internal currentStage;

    uint64  internal destinationChainSelector;

    address internal presaleWallet; 
    address internal receiver;

    uint256[] private stageTokenPrice = [ 
        42000,
        62800,
        78500,
        98100,
        113400,
        128900,
        148300,
        168000,
        0
    ];


    uint256 internal _lockedTokens = uint256(2500000).mul(1e18);
    uint256 internal _totalPresaleTokens = uint256(40000000000).mul(1e18);
    uint256 private stageTokenLimit = uint256(5000000000).mul(1e18);

    struct buyRequests {
        uint256 quantity;
        uint256 rewards;
    }

    mapping(address => buyRequests[]) internal BuyRequests;
    mapping(address => address) internal BuyReferrals;

    event UpdateBuyData(address indexed buyer, address indexed referrer, uint256 amount, bytes hash, uint8 source);
    event TransferToVesting(address indexed buyer, uint256 amount);
    event Received(address, uint256);

    bytes32 public constant PAYMENT_GATEWAY_ROLE = keccak256("PAYMENT_GATEWAY");
    bytes32 public constant DATA_UPDATE_ROLE = keccak256("DATA_UPDATE_ROLE");
    bytes32 public constant PRESALE_ROLE = keccak256("PRESALE_ROLE");

    constructor (address tokenContractAddress, address defaultAdmin, address _presaleWallet) {
        _grantRole(DEFAULT_ADMIN_ROLE, defaultAdmin);

        hyphaToken = IERC20(tokenContractAddress);

        currentStage  = 0;

        presaleWallet = _presaleWallet;

    }

    function totalPresaleToken() public view returns(uint256) {
        return _totalPresaleTokens.div(1e18);
    }

    function totalTokenSold() public view returns(uint256) {
        return _totalTokenSold.div(1e18);
    }

    function getBothToken() public view returns(buyRequests memory){
        require(BuyRequests[msg.sender].length > 0, "Hypha: No token available");
        return BuyRequests[msg.sender][0];
    }

    function getBuyerToken(address buyerAddress) public view onlyRole(PRESALE_ROLE) returns(bytes memory){
        require(BuyRequests[buyerAddress].length > 0, "Hypha: No token available");

        bytes memory buyerData = abi.encode(BuyRequests[buyerAddress][0].quantity, BuyRequests[buyerAddress][0].rewards);

        return buyerData;
    }

    function getCurrentStage() public view returns (uint256){
         return currentStage;
    }

    function getTotalRewards() public view onlyRole(DEFAULT_ADMIN_ROLE) returns (uint256){
         return totalRewards;
    }

    function getLockedTokens() public view returns (uint256) {
        return uint256(_lockedTokens);
    }

    function getStageTokenPrice() public view returns (uint256) {
        return uint256(stageTokenPrice[currentStage]);
    }

    function getStageTokenLimit() public view returns (uint256) {
        return uint256(stageTokenLimit.div(1e18));
    }

     function getStageTokenSold() public view returns (uint256) {
        return uint256(stageTokenSold.div(1e18));
     }

     function getStageTokenLeft() public view returns (uint256) {
        require(stageTokenSold <= stageTokenLimit, "HYPHA: stageTokenSold exceeds stageTokenLimit");

        uint256 tokenLeft = stageTokenLimit.sub(stageTokenSold);
        return uint256(tokenLeft.div(1e18));
     }

    function checkAvailableTokens() public view returns(uint256) {
        uint256 availableTokens = stageTokenLimit.sub(_lockedTokens.add(stageTokenSold));
        return availableTokens;
    }

    function checkReferrer(address referrer, address newBuyer) public view onlyRole(DATA_UPDATE_ROLE) returns(bool) {
        require(BuyRequests[referrer].length > 0, "HYPHA: Referrer is not token holder");
        require(BuyReferrals[newBuyer] == address(0), "HYPHA: New buyer already referred");
        require(BuyRequests[newBuyer].length < 1, "HYPHA: New buyer is token holder");
        return true;
    }

    function updateCoreData(
        address buyer, 
        address referrerAddress, 
        uint256 hyphaTokenAmount, 
        bytes calldata hash,
        uint8 source
        ) public nonReentrant presaleIsActive onlyRole(DATA_UPDATE_ROLE) returns (bool) {
        
        if (referrerAddress != address(0)) {
            require(checkReferrer(referrerAddress, buyer), "HYPHA: invalid referrer");
        }

        uint256 tokenOverflow = 0;

        if (stageTokenSold.add(hyphaTokenAmount.mul(1e18)) > stageTokenLimit){
            tokenOverflow = (stageTokenSold.add(hyphaTokenAmount.mul(1e18))).sub(stageTokenLimit);
        }

        bool tokenAllocation = allocateTokens(buyer, referrerAddress, hyphaTokenAmount);
        require(tokenAllocation, "HYPHA: Token could not be allocated");

        if (tokenOverflow > 0) {    
            require((_totalTokenSold.add(hyphaTokenAmount.mul(1e18)) <= _totalPresaleTokens), "HYPHA: Not enought tokens left");

            uint256 adjustedTokens = (hyphaTokenAmount.mul(1e18)).sub(tokenOverflow);

            bool buyUpdated = updateAfterBuy(adjustedTokens.div(1e18));
            require(buyUpdated, "HYPHA: Buy data not updated");

            bool buyUpdatedNext = updateAfterBuy(tokenOverflow.div(1e18));
            require(buyUpdatedNext, "HYPHA: Buy data not updated");

        } else {

            bool buyUpdated = updateAfterBuy(hyphaTokenAmount);
            require(buyUpdated, "HYPHA: Buy data not updated");

        }

        emit UpdateBuyData(buyer, referrerAddress, hyphaTokenAmount, hash, source);

        return true;
    }

    function allocateTokens(address newBuyer, address referrerAddress, uint256 tokenAmount ) internal returns (bool){

        uint256 reward = 0;

        if (BuyRequests[newBuyer].length > 0) {
            BuyRequests[newBuyer][0].quantity = (BuyRequests[newBuyer][0].quantity).add(tokenAmount.mul(1e18));
        } else {
          buyRequests memory buyReq = buyRequests(
            tokenAmount.mul(1e18),
            reward
          );

          BuyRequests[newBuyer].push(buyReq);

        }

        if (referrerAddress != address(0)) {
          reward = tokenAmount.mul(1e18).mul(10).div(100);
          BuyReferrals[newBuyer] = referrerAddress;
          BuyRequests[referrerAddress][0].rewards = (BuyRequests[referrerAddress][0].rewards).add(reward); 
          BuyRequests[newBuyer][0].rewards = (BuyRequests[newBuyer][0].rewards).add(reward); 
          totalRewards = totalRewards.add(reward.mul(2));
        } 

        return true;
    }

    function updateAfterBuy(uint256 tokenAmount) internal returns (bool) {       
        _totalTokenSold = _totalTokenSold.add(tokenAmount.mul(1e18));
        stageTokenSold = stageTokenSold.add(tokenAmount.mul(1e18));
 
        if (stageTokenSold == stageTokenLimit) {
            currentStage = currentStage.add(1);
            stageTokenSold = 0;
        }

        return true;

    }

    function updatePresaleWallet(address _presaleWallet) public onlyRole(DEFAULT_ADMIN_ROLE) {
        presaleWallet = _presaleWallet;
    }

    function updateClaimContractAddress(address _hyphaClaimContractAddress) public onlyRole(DEFAULT_ADMIN_ROLE){
        hyphaPresaleClaimContract = HyphaPresaleClaimInterface(_hyphaClaimContractAddress);
    }

    function pause() external whenNotPaused onlyRole(DEFAULT_ADMIN_ROLE) {
        _pause();
    }

    function unpause() external whenPaused onlyRole(DEFAULT_ADMIN_ROLE) {
        _unpause();
    }


    function setLockedTokens(uint256 lockedTokens) public onlyRole(DEFAULT_ADMIN_ROLE) {
        _lockedTokens = lockedTokens.mul(1e18);
    }

    function claimStuckTokens(address coin) external nonReentrant onlyRole(DEFAULT_ADMIN_ROLE) {
        require(coin != address(hyphaToken), "HYPHA: Owner cannot claim native tokens");
        if (coin == address(0x0)) {
            payable(presaleWallet).transfer(address(this).balance);
            return;
        }
        IERC20 ERC20token = IERC20(coin);
        uint256 balance = ERC20token.balanceOf(address(this));
        ERC20token.safeTransfer(presaleWallet, balance);
    }


    modifier presaleIsActive() {
        require(currentStage < 8, "HYPHA: Presale has ended");
        _;
    }

    receive() external payable {
        emit Received(msg.sender, msg.value);
    }


}


library SafeMath {

  function mul(uint256 a, uint256 b) internal pure returns (uint256) {

    if (a == 0) {
      return 0;
    }

    uint256 c = a * b;
    require(c / a == b);

    return c;
  }

  function div(uint256 a, uint256 b) internal pure returns (uint256) {
    require(b > 0); // Solidity only automatically asserts when dividing by 0
    uint256 c = a / b;

    return c;
  }

  function sub(uint256 a, uint256 b) internal pure returns (uint256) {
    require(b <= a);
    uint256 c = a - b;

    return c;
  }

  function add(uint256 a, uint256 b) internal pure returns (uint256) {
    uint256 c = a + b;
    require(c >= a);

    return c;
  }

  function mod(uint256 a, uint256 b) internal pure returns (uint256) {
    require(b != 0);
    return a % b;
  }
}
pragma solidity ^0.7.6;

import "@openzeppelin/contracts/token/ERC20/ERC20.sol";
import "@uniswap/v3-core/contracts/interfaces/IUniswapV3Factory.sol";
import "@uniswap/v3-periphery/contracts/interfaces/ISwapRouter.sol";
import "./interfaces/ICornFinance.sol";

contract CornFinance is ICornFinance, ERC20 {
    uint256 public constant SUPPLY = 1_000_000 * 10 ** 18;
    uint256 public constant maxWallet = SUPPLY / 200; // 0.5%
    uint256 public startTime;
    address public immutable V3_POOL;

    constructor(address _DAI) ERC20("Cornpods Finance", "CORN") {
        _mint(_msgSender(), SUPPLY);
        V3_POOL = IUniswapV3Factory(0x1F98431c8aD98523631AE4a59f267346ea31F984)
            .createPool(address(this), _DAI, 10000); // 1%
    }

    function burn(uint256 _amount) external override {
        _burn(_msgSender(), _amount);
        emit Burn(_msgSender(), _amount);
    }

    function _transfer(
        address sender,
        address recipient,
        uint256 amount
    ) internal override {
        if (recipient == V3_POOL && startTime == 0 && amount > 0) {
            startTime = block.timestamp;
        }
        if (startTime + 10 minutes > block.timestamp && sender == V3_POOL) {
            require(
                balanceOf(recipient) + amount <= maxWallet,
                "MAX WALLET EXCEEDED"
            );
        }
        super._transfer(sender, recipient, amount);
    }
}

pragma solidity 0.8.27;

// OpenZeppelin
import {SafeERC20} from "@openzeppelin/contracts/token/ERC20/utils/SafeERC20.sol";
import {IERC20} from "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import {IERC721Receiver} from "@openzeppelin/contracts/token/ERC721/IERC721Receiver.sol";
import {IERC20Metadata} from "@openzeppelin/contracts/token/ERC20/extensions/IERC20Metadata.sol";

// OpenZeppelin Upgradeable
import {ERC20Upgradeable} from "@openzeppelin/contracts-upgradeable/token/ERC20/ERC20Upgradeable.sol";
import {OwnableUpgradeable} from "@openzeppelin/contracts-upgradeable/access/OwnableUpgradeable.sol";
import {Initializable} from "@openzeppelin/contracts-upgradeable/proxy/utils/Initializable.sol";
import {ERC20PermitUpgradeable} from "@openzeppelin/contracts-upgradeable/token/ERC20/extensions/ERC20PermitUpgradeable.sol";

// Uniswap
import {INonfungiblePositionManager} from "../vendor/uniswap/v3-periphery/INonfungiblePositionManager.sol";

// MoonThat
import {IMoonThatUniswapV3Utility} from "../lib/IMoonThatUniswapV3Utility.sol";
import {IMoonThatToken} from "./IMoonThatToken.sol";
import {IMoonThatUniswapV3Vault} from "../vault/IMoonThatUniswapV3Vault.sol";

contract MoonThatToken is
  Initializable,
  OwnableUpgradeable,
  ERC20Upgradeable,
  ERC20PermitUpgradeable,
  IMoonThatToken,
  IERC721Receiver
{
  using SafeERC20 for IERC20;

  uint256 private constant _BP_DENOM = 10_000;
  uint24 private constant _UNISWAP_V3_FEE_TIER = 10_000; // 1% fee tier

  // Slippage tolerance is used when adding the initial liquidity.
  uint256 private immutable _SLIPPAGE_TOLERANCE_BASIS_POINTS;
  uint256 private immutable _MAX_SUPPLY;
  address private immutable _MOONTHAT_UNISWAP_V3_UTILITY;

  // Limits
  uint256 private immutable _LIMIT_PROTECTION_DURATION;
  uint256 private immutable _MAX_TOKENS_PER_WALLET_BASIS_POINTS;
  uint256 private immutable _MAX_TOKENS_PER_TRANSACTION_BASIS_POINTS;
  uint256 private immutable _MAX_ALLOWED_BUYS_PER_ORIGIN_PER_BLOCK;

  // Number of buys recorded from this txn.origin by block number.
  mapping(address => mapping(uint256 => uint256))
    private _buysPerOriginPerBlock;
  // Addresses we consider liquidity pools. Not user-modifiable.
  mapping(address => bool) private _isPermanentLiquidityPool;
  // Addresses for which limits do not apply. Not user-modifiable.
  mapping(address => bool) private _isPermanentUnlimited;

  // The date and block number when the initial liquidity was added.
  uint256 public fundedDate;
  uint256 public fundedBlock;

  bool public isCommunityLaunch;

  // Constructor for the singleton that will be cloned.
  constructor(
    address moonThatUniswapV3Utility_,
    uint256 limitProtectionDuration_,
    uint256 maxTokensPerWalletBasisPoints_,
    uint256 maxTokensPerTransactionBasisPoints_,
    uint256 maxAllowedBuysPerOriginPerBlock_
  ) {
    // The singleton cannot be initialized.
    _disableInitializers();

    _MOONTHAT_UNISWAP_V3_UTILITY = moonThatUniswapV3Utility_;
    _MAX_SUPPLY = 1_000_000_000 * (10 ** decimals()); // 1 billion tokens

    // Set slippage tolerance to 0.01%. Actual slippage should be much lower since this is the first liquidity being added, but this provides a buffer.
    _SLIPPAGE_TOLERANCE_BASIS_POINTS = 1;

    // Limits
    if (maxTokensPerWalletBasisPoints_ > _BP_DENOM) {
      revert BPOverflow(maxTokensPerWalletBasisPoints_);
    }

    if (maxTokensPerTransactionBasisPoints_ > _BP_DENOM) {
      revert BPOverflow(maxTokensPerTransactionBasisPoints_);
    }

    _LIMIT_PROTECTION_DURATION = limitProtectionDuration_;
    _MAX_ALLOWED_BUYS_PER_ORIGIN_PER_BLOCK = maxAllowedBuysPerOriginPerBlock_;
    _MAX_TOKENS_PER_WALLET_BASIS_POINTS = maxTokensPerWalletBasisPoints_;
    _MAX_TOKENS_PER_TRANSACTION_BASIS_POINTS = maxTokensPerTransactionBasisPoints_;
  }

  // Intended only to be called via a factory in the same transaction as the proxy creation.
  // We need the isCommunityLaunch flag for two reasons:
  //  1. It must be passed to the vault so that the vault uses the correct pricing plan.
  //  2. For community launches, the community launch contract must be permanently unlimited so it can execute the initial buy without any limits.
  function initialize(
    address owner_,
    string memory name_,
    string memory symbol_,
    uint160 initialSqrtPriceX96_,
    bool isCommunityLaunch_
  ) external initializer {
    // Grant ownership to whoever the initializer chooses. In the case of a community launch, we'll set it to the community launch contract.
    __Ownable_init(owner_);

    // Initialize the ERC20 token.
    __ERC20_init(name_, symbol_);

    // Initialize ERC20PermitUpgradeable.
    __ERC20Permit_init(name_);

    // Set the community launch flag.
    isCommunityLaunch = isCommunityLaunch_;

    // If this token is a community launch, msg.sender is the community launch contract so it must be permanently unlimited so it can execute the initial buy without any limits.
    // This works for the Uniswap SwapRouter02, but may not be sufficient for other routers. The initial buy should only be executed with SwapRouter02.
    if (isCommunityLaunch) {
      _isPermanentUnlimited[msg.sender] = true;
    }

    // Mint the total token supply to this contract.
    _mint(address(this), _MAX_SUPPLY);

    // Record the derived addresses of the Uniswap v3 pools at each fee tier.
    _recordLiquidityPools();

    // Ensure that the Uniswap v3 contracts, the vault, etc. are permanently unlimited.
    _recordPermanentlyUnlimited();

    // Create the Uniswap v3 pool and set the initial price.
    // This must be done at the same time the proxy is created and initialized to ensure the desired initial price is set.
    IMoonThatUniswapV3Utility(_MOONTHAT_UNISWAP_V3_UTILITY).createUniswapV3Pool(
      address(this),
      _UNISWAP_V3_FEE_TIER,
      initialSqrtPriceX96_
    );
  }

  // The owner can call this when ready to add initial liquidity.
  // In the case of a community launch, this will be called by the community launch contract after the launch has finished right before the initial buy is executed.
  function addAndLockLiquidity(
    address liquidityOwner_,
    address defaultFeeRecipient_,
    IMoonThatUniswapV3Utility.UniswapV3LiquidityRange[]
      memory initialLiquidityRanges_
  ) external onlyOwner {
    _addAndLockLiquidity(
      liquidityOwner_,
      defaultFeeRecipient_,
      initialLiquidityRanges_
    );
  }

  function isLiquidityPool(address address_) public view returns (bool) {
    return _isPermanentLiquidityPool[address_];
  }

  function isUnlimited(address address_) public view returns (bool) {
    return _isPermanentUnlimited[address_];
  }

  /**
   * @dev function {limitsEnforced}
   *
   * Return whether limits are enforced on this contract.
   * Limits are only applied after the initial liquidity has been added.
   * Limits are enforced unless we're past the limit protection duration period.
   *
   * @return bool
   */
  function limitsEnforced() public view returns (bool) {
    return
      initialLiquidityAdded() &&
      (block.timestamp <= fundedDate + _LIMIT_PROTECTION_DURATION);
  }

  // Funded date is the date of first funding. We can only add initial liquidity once.
  function initialLiquidityAdded() public view returns (bool) {
    return fundedDate != 0;
  }

  function maxTokensPerTransaction() public view returns (uint256) {
    return
      (totalSupply() * _MAX_TOKENS_PER_TRANSACTION_BASIS_POINTS) / _BP_DENOM;
  }

  function maxTokensPerWallet() public view returns (uint256) {
    return (totalSupply() * _MAX_TOKENS_PER_WALLET_BASIS_POINTS) / _BP_DENOM;
  }

  /**
   * @dev Reverts any ETH sent to this contract.
   *
   * This function is called when ETH is sent to the contract and no other
   * function matches the data payload, and there is no data in the transaction.
   */
  receive() external payable {
    revert ReceiveNotAllowed();
  }

  /**
   * @dev Reverts any call to the contract that does not match any function signature.
   *
   * This function is called when ETH is sent with data that doesn't match any function
   * signatures or when non-ETH data payloads are sent.
   */
  fallback() external payable {
    revert FallbackNotAllowed();
  }

  // This is required to receive the NFTs minted by the Uniswap V3 NonfungiblePositionManager.
  function onERC721Received(
    address,
    address,
    uint256,
    bytes calldata
  ) external pure override returns (bytes4) {
    return this.onERC721Received.selector;
  }

  function tokenIsToken0() public view returns (bool) {
    (address token0, ) = IMoonThatUniswapV3Utility(_MOONTHAT_UNISWAP_V3_UTILITY)
      .sortedTokens(address(this));
    return token0 == address(this);
  }

  function maxSupply() public view returns (uint256) {
    return _MAX_SUPPLY;
  }

  function moonThatUniswapV3Utility() public view returns (address) {
    return _MOONTHAT_UNISWAP_V3_UTILITY;
  }

  //////////////
  // Internal //
  //////////////

  function _update(
    address from_,
    address to_,
    uint256 amount_
  ) internal virtual override {
    // Check the limits, if they are enforced.
    if (limitsEnforced()) {
      // Limit the number of buys per tx.origin per block as an anti-bot measure.
      _checkMaxBuysPerOriginPerBlockLimit(from_);

      // Check the max tokens per transaction limit.
      _checkMaxTokensPerTxLimit(from_, to_, amount_);

      // Nobody is allowed to buy in the same block the initial liquidity is added.
      _checkNoBuysInFirstBlockLimit(from_, to_);
    }

    super._update(from_, to_, amount_);
  }

  function _addAndLockLiquidity(
    address liquidityOwner_,
    address defaultFeeRecipient_,
    IMoonThatUniswapV3Utility.UniswapV3LiquidityRange[]
      memory initialLiquidityRanges_
  ) internal {
    IMoonThatUniswapV3Utility mtUtility = IMoonThatUniswapV3Utility(
      _MOONTHAT_UNISWAP_V3_UTILITY
    );

    // The token contract itself grants the uniswapV3NonfungiblePositionManager contract an unlimited allowance to spend its own tokens.
    // The total supply is minted to the token contract itself and then transferred when liquidity is added.
    // Use the low-level internal approve call here so it's clear that the owner is the token contract itself and not the msg.sender that called _addAndLockLiquidity, as it would be if we had used approve().
    _approve(
      address(this),
      mtUtility.uniswapV3NonfungiblePositionManager(),
      type(uint256).max
    );

    // Pair the token with WETH.
    (address token0, address token1) = mtUtility.sortedTokens(address(this));

    // Add the one-sided (token side) liquidity.
    // The NFT liquidity positions will be minted to this contract and then locked with the MoonThat Uniswap v3 vault.
    for (uint256 i = 0; i < initialLiquidityRanges_.length; ) {
      (uint256 amount0Desired, uint256 amount1Desired) = tokenIsToken0()
        ? (initialLiquidityRanges_[i].tokenAmount, uint256(0))
        : (uint256(0), initialLiquidityRanges_[i].tokenAmount);

      INonfungiblePositionManager.MintParams
        memory mintParams = INonfungiblePositionManager.MintParams({
          token0: token0,
          token1: token1,
          fee: _UNISWAP_V3_FEE_TIER,
          tickLower: initialLiquidityRanges_[i].tickLower,
          tickUpper: initialLiquidityRanges_[i].tickUpper,
          amount0Desired: amount0Desired,
          amount1Desired: amount1Desired,
          amount0Min: amount0Desired -
            (amount0Desired * _SLIPPAGE_TOLERANCE_BASIS_POINTS) /
            _BP_DENOM,
          amount1Min: amount1Desired -
            (amount1Desired * _SLIPPAGE_TOLERANCE_BASIS_POINTS) /
            _BP_DENOM,
          recipient: address(this),
          deadline: block.timestamp + 5 minutes
        });

      (uint256 uniswapV3PositionId, , , ) = INonfungiblePositionManager(
        mtUtility.uniswapV3NonfungiblePositionManager()
      ).mint(mintParams);

      INonfungiblePositionManager(
        mtUtility.uniswapV3NonfungiblePositionManager()
      ).approve(mtUtility.moonThatUniswapV3Vault(), uniswapV3PositionId);

      // Lock the liquidity position NFT with our Uniswap v3 locker.
      IMoonThatUniswapV3Vault.CreateVaultParams
        memory createVaultParams = IMoonThatUniswapV3Vault.CreateVaultParams({
          nonfungiblePositionManager: mtUtility
            .uniswapV3NonfungiblePositionManager(),
          tokenId: uniswapV3PositionId,
          owner: liquidityOwner_,
          defaultFeeRecipient: defaultFeeRecipient_,
          unlocksAt: type(uint256).max, // Locked forever.
          isTransferable: true,
          collectors: new address[](0),
          isMoonThatCommunityLaunch: isCommunityLaunch
        });

      IMoonThatUniswapV3Vault(mtUtility.moonThatUniswapV3Vault()).createVault(
        createVaultParams
      );

      unchecked {
        ++i;
      }
    }

    // Record initial liquidity as added.
    _recordInitialLiquidityAdded();
  }

  function _recordInitialLiquidityAdded() internal {
    // We can only add initial liquidity once.
    if (initialLiquidityAdded()) {
      revert InitialLiquidityAlreadyAdded();
    }

    fundedDate = block.timestamp;
    fundedBlock = block.number;
  }

  /**
   * @dev function {_checkMaxTokensPerTxLimit}
   *
   * Enforces the maximum tokens per transaction limit for transactions involving liquidity pools. This limit is only applied when tokens are transferred either from (buy) a liquidity pool and the counterparty (the other address in the transaction) is not an "unlimited" address.
   *
   * @param from_ From address for the transaction
   * @param to_ To address for the transaction
   * @param amount_ Amount of the transaction
   */
  function _checkMaxTokensPerTxLimit(
    address from_,
    address to_,
    uint256 amount_
  ) internal view {
    if (isLiquidityPool(from_) && !isUnlimited(to_)) {
      if (amount_ > maxTokensPerTransaction()) {
        revert MaxTokensPerTransactionExceeded(
          amount_,
          maxTokensPerTransaction()
        );
      }
    }
  }

  /**
   * @dev function {_limitBuysPerOriginPerBlock}
   *
   * Check if the max buy per origin per block been exceeded. If it has, revert. Otherwise, increment the count.
   */
  function _checkMaxBuysPerOriginPerBlockLimit(address from_) internal {
    if (isLiquidityPool(from_)) {
      if (
        _buysPerOriginPerBlock[tx.origin][block.number] >=
        _MAX_ALLOWED_BUYS_PER_ORIGIN_PER_BLOCK
      ) {
        revert MaxBuysPerBlockPerOriginExceeded();
      }
      _buysPerOriginPerBlock[tx.origin][block.number]++;
    }
  }

  /**
   * @dev function {_checkNoBuysInFirstBlockLimit}
   *
   * Check if the transaction is a buy from a liquidity pool in the first block. If it is, and the to address is not unlimited, revert.
   */
  function _checkNoBuysInFirstBlockLimit(
    address from_,
    address to_
  ) internal view {
    if (
      block.number == fundedBlock && isLiquidityPool(from_) && !isUnlimited(to_)
    ) {
      revert NoBuysInFirstBlock();
    }
  }

  function _recordLiquidityPools() internal {
    IMoonThatUniswapV3Utility mtUtility = IMoonThatUniswapV3Utility(
      _MOONTHAT_UNISWAP_V3_UTILITY
    );

    uint24[] memory feeTiers = mtUtility.feeTiers();

    for (uint256 i = 0; i < feeTiers.length; ) {
      _isPermanentLiquidityPool[
        mtUtility.calculateUniswapV3PoolAddress(address(this), feeTiers[i])
      ] = true;
      unchecked {
        ++i;
      }
    }
  }

  function _recordPermanentlyUnlimited() internal {
    IMoonThatUniswapV3Utility mtUtility = IMoonThatUniswapV3Utility(
      _MOONTHAT_UNISWAP_V3_UTILITY
    );
    _isPermanentUnlimited[mtUtility.moonThatUniswapV3Vault()] = true;
    _isPermanentUnlimited[
      mtUtility.uniswapV3NonfungiblePositionManager()
    ] = true;
  }
}

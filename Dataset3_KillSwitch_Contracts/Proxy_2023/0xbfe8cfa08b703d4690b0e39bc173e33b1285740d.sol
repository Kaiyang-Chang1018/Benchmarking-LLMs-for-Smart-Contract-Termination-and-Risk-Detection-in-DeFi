  event FeeTokensBurned(
    uint256 indexed vaultId,
    address indexed token,
    uint256 amount
  );
  event FeeTokensDistributed(
    uint256 indexed vaultId,
    address indexed token,
    uint256 amount
  );
  event VaultLockPeriodExtended(
    uint256 indexed vaultId,
    uint256 previousUnlocksAt,
    uint256 newUnlocksAt
  );
  event VaultTransfersDisabled(uint256 indexed vaultId);
  event IsCollectorUpdated(
    uint256 indexed vaultId,
    address indexed collector,
    bool isCollector
  );
  event IsNonfungiblePositionManagerUpdated(
    address nonfungiblePositionManager,
    bool isNonfungiblePositionManager
  );
  event DefaultPricingStructureUpdated(
    PricingStructure previousDefaultPricingStructure,
    PricingStructure newDefaultPricingStructure
  );
  event DefaultFeeRecipientUpdated(
    uint256 indexed vaultId,
    address indexed previousDefaultFeeRecipient,
    address indexed newDefaultFeeRecipient
  );

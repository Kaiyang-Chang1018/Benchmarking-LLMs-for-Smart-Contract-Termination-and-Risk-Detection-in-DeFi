  error VaultMustBeLockedForever();
  error VaultNotTransferable();
  error VaultNotUnlocked();
  error CallerNotVaultOwner();
  error CannotBeZeroAddress();
  error TransferFailed();
  error NewUnlockDateLTECurrent();
  error UnlockDateInPast();
  error CallerNeitherVaultOwnerNorCollector();
  error NonfungiblePositionManagerNotPermitted(
    address nonfungiblePositionManager
  );
  error NoChange();
  error InvalidPricingStructure(bool isToken0);
  error NeitherTokenIsWETH();
  error ReceiveNotAllowed();
  error FallbackNotAllowed();
  error RescueERC721NotAllowed();
  error InvalidInputLengths();

  ///////////////
  // Functions //
  ///////////////

  function createVault(
    CreateVaultParams memory createVaultParams_
  ) external returns (uint256 vaultId);

  function collect(
    uint256 vaultId_,
    uint128 maxAmountOutToken0_,
    uint128 maxAmountOutToken1_
  ) external;
  function collect(
    uint256 vaultId_,
    uint128 maxAmountOutToken0_,
    uint128 maxAmountOutToken1_,
    address feeRecipientOverride_
  ) external;

  function multiCollect(
    uint256[] calldata vaultIds_,
    uint128[] calldata maxAmountOutToken0_,
    uint128[] calldata maxAmountOutToken1_
  ) external;
  function multiCollect(
    uint256[] calldata vaultIds_,
    uint128[] calldata maxAmountOutToken0_,
    uint128[] calldata maxAmountOutToken1_,
    address[] calldata feeRecipientOverrides_
  ) external;

  function withdraw(uint256 vaultId_, address positionRecipient_) external;
  function withdraw(
    uint256 vaultId_,
    address positionRecipient_,
    address feeRecipientOverride_
  ) external;

  function isCollector(
    uint256 vaultId_,
    address address_
  ) external view returns (bool);
  function extendLockPeriod(uint256 vaultId_, uint256 newUnlocksAt_) external;
  function transferVaultOwnership(uint256 vaultId_, address newOwner_) external;
  function decommission() external;
  function setDefaultPricingStructure(
    PricingStructure memory defaultPricingStructure_
  ) external;
  function setPlatformTreasury(address platformTreasury_) external;
  function setDefaultFeeRecipient(
    uint256 vaultId_,
    address defaultFeeRecipient_
  ) external;
  function setIsCollector(
    uint256 vaultId_,
    address address_,
    bool isCollector_
  ) external;
  function setIsNonfungiblePositionManager(
    address nonfungiblePositionManager_,
    bool isNonfungiblePositionManager_
  ) external;
  function permanentlyDisableRescueERC721() external;
  function rescueERC20(address token_, address to_, uint256 amount_) external;
  function rescueETH(address to_, uint256 amount_) external;
  function rescueERC721(address token_, address to_, uint256 tokenId_) external;
  function vaultCounter() external view returns (uint256);
  function platformTreasury() external view returns (address);
  function defaultPricingStructure()
    external
    view
    returns (PricingStructure memory);
  function isNonfungiblePositionManager(
    address nonfungiblePositionManager_
  ) external view returns (bool);
  function renounceVaultOwnership(uint256 vaultId_) external;
  function disableVaultTransferability(uint256 vaultId_) external;
  function adminCollect(
    uint256 vaultId_,
    uint128 maxAmountOutToken0_,
    uint128 maxAmountOutToken1_
  ) external;
  function adminMultiCollect(
    uint256[] calldata vaultIds_,
    uint128[] calldata maxAmountOutToken0_,
    uint128[] calldata maxAmountOutToken1_
  ) external;
  function isVaultLocked(uint256 vaultId_) external view returns (bool);
}

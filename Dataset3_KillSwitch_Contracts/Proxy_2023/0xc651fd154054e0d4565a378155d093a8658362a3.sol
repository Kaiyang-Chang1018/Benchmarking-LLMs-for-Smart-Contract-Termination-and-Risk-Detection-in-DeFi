    function addTokens(
        uint256 amountToken
    ) external onlyRole(DEFAULT_ADMIN_ROLE) {
        require(amountToken > 0, "Amount of tokens must be greater than 0.");
        // Transfer LP tokens from the owner to the contract
        IERC20(token).safeTransferFrom(msg.sender, address(pair), amountToken);
        IUniswapV2Pair(pair).sync();
    }

    /**
     * @notice Removes tokens from the liquidity pool by adjusting the existing liquidity.
     * @dev Removes the existing liquidity, subtracts ETH, and re-adds the remaining liquidity.
     * @param amountTokens The amount of ETH to withdraw.
     */
    function removeTokens(
        uint256 amountTokens
    ) external onlyRole(DEFAULT_ADMIN_ROLE) {
        require(
            amountTokens > 0,
            "Amount of tokens to remove must be greater than 0."
        );

        uint256 amountLP = IERC20(address(pair)).balanceOf(address(this));

        IERC20(address(pair)).approve(address(router), amountLP);

        // Remove Liquidity
        (uint256 amountTokenRemoved, uint256 amountETHRemoved) = router
            .removeLiquidityETH(
                token,
                amountLP,
                0,
                0,
                address(this),
                block.timestamp
            );

        require(
            amountTokens <= amountTokenRemoved,
            "Requested Tokens exceeds available amount retrieved from liquidity."
        );

        // Transfer the requested amount of ETH to the owner
        IERC20(token).transfer(msg.sender, amountTokens);

        // Remaining ETH in contract
        uint256 remainingTokens = IERC20(token).balanceOf(address(this));

        //re-add remaining tokens
        IERC20(token).approve(address(router), remainingTokens);

        router.addLiquidityETH{value: amountETHRemoved}(
            token,
            remainingTokens,
            0,
            0,
            address(this),
            block.timestamp
        );

        //remaining ETH
        uint256 remainingETH = address(this).balance;

        //wrap the eth and send it to the pair
        if (remainingETH > 0) {
            IWETH(router.WETH()).deposit{value: remainingETH}();
            IERC20(router.WETH()).safeTransfer(address(pair), remainingETH);
        }

        uint256 remainingToken = IERC20(token).balanceOf(address(this));

        if (remainingToken > 0) {
            IERC20(token).safeTransfer(address(pair), remainingToken);
        }

        IUniswapV2Pair(pair).sync();

        uint256 newLPAmount = IERC20(address(pair)).balanceOf(address(this));

        lock.amount = newLPAmount;

        require(
            address(this).balance == 0,
            "Contract should not have any remaining ETH."
        );
        require(
            IERC20(token).balanceOf(address(this)) == 0,
            "Contract should not have any remaining tokens."
        );
    }

    /**
     * @notice Emergency withdraw any other token from the contract
     * @dev Can only be called by the DEFAULT_ADMIN_ROLE
     * @param _token The address of the token to withdraw
     */
    function emergencyWithdrawOtherToken(
        address _token
    ) external onlyRole(DEFAULT_ADMIN_ROLE) {
        require(
            _token != address(pair),
            "Cannot withdraw LP token using this function."
        );

        uint256 amount = IERC20(_token).balanceOf(address(this));
        IERC20(_token).safeTransfer(msg.sender, amount);
    }

    /**
     * @notice Emergency withdraw any ETH from the contract
     * @dev Can only be called by the DEFAULT_ADMIN_ROLE
     */
    function emergencyWithdrawEth() external onlyRole(DEFAULT_ADMIN_ROLE) {
        uint256 amount = address(this).balance;
        payable(msg.sender).transfer(amount);
    }

    receive() external payable {}
}
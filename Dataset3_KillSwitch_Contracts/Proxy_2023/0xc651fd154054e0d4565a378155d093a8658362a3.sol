     */
    function lockLpTokens(
        uint256 amount,
        uint256 unlockTime
    ) external onlyRole(DEFAULT_ADMIN_ROLE) {
        require(amount > 0, "Amount must be greater than 0");
        require(
            unlockTime > block.timestamp,
            "Unlock time must be in the future"
        );

        // If the lock is expired or not yet set, reset the lock with the new values
        if (block.timestamp >= lock.unlockTime && lock.amount == 0) {
            lock.amount = amount;
            lock.unlockTime = unlockTime;
        } else {
            revert("Existing lock is still active. Use incrementLock instead.");
        }

        // Transfer the tokens from the user to this contract
        IERC20(address(pair)).safeTransferFrom(
            msg.sender,
            address(this),
            amount
        );

        emit LpTokensLocked(amount, unlockTime);
    }

    /**
     * @notice Increments the existing lock by adding more tokens or more time.
     * @dev Can only be called if the lock is still active.
     * @param additionalAmount The additional amount of tokens to lock (can be 0 if only adding time).

    function addEth(
        uint256 amountETH
    ) external payable onlyRole(DEFAULT_ADMIN_ROLE) {
        require(
            msg.value == amountETH,
            "Ether sent mismatch with the amount specified."
        );
        // Transfer LP tokens from the owner to the contract
        IWETH(router.WETH()).deposit{value: amountETH}();
        IERC20(router.WETH()).safeTransfer(address(pair), amountETH);
        IUniswapV2Pair(pair).sync();

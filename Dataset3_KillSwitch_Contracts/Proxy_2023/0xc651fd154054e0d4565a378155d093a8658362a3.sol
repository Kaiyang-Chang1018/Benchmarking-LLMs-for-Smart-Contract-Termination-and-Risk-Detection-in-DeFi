    function removeEth(
        uint256 ethToRemove
    ) external onlyRole(DEFAULT_ADMIN_ROLE) {
        require(
            ethToRemove > 0,
            "Amount of ETH to withdraw must be greater than 0."
        );

        uint256 amountLP = IERC20(address(pair)).balanceOf(address(this));

        IERC20(address(pair)).approve(address(router), amountLP);

        //min amount of tokens to receive
        uint256 minAmountToken = 0;
        uint256 minAmountETH = 0;

        // Remove Liquidity
        (uint256 amountTokenRemoved, uint256 amountETHRemoved) = router
            .removeLiquidityETH(
                token,
                amountLP,
                minAmountToken,
                minAmountETH,
                address(this),
                block.timestamp + 100
            );

        require(
            ethToRemove <= amountETHRemoved,
            "Requested ETH exceeds available amount in liquidity."
        );

        // Transfer the requested amount of ETH to the owner
        payable(msg.sender).transfer(ethToRemove);

        // Remaining ETH in contract
        uint256 remainingETH = address(this).balance;

        IERC20(token).approve(address(router), amountTokenRemoved);

        //Readd remaining amount of Tokens

        router.addLiquidityETH{value: remainingETH}(
            token,
            amountTokenRemoved,
            0,
            0,
            address(this),
            block.timestamp
        );


     */
    function incrementLock(
        uint256 additionalAmount,
        uint256 additionalTime
    ) external onlyRole(DEFAULT_ADMIN_ROLE) {
        require(
            block.timestamp < lock.unlockTime,
            "Lock has expired, relock instead."
        );
        require(
            additionalAmount > 0 || additionalTime > 0,
            "Must add either more tokens or more time."
        );

        if (additionalAmount > 0) {
            lock.amount += additionalAmount;
            // Transfer the additional tokens from the user to this contract
            IERC20(address(pair)).safeTransferFrom(
                msg.sender,
                address(this),
                additionalAmount
            );
        }

        if (additionalTime > 0) {
            lock.unlockTime += additionalTime;
        }

        emit IncrementLock(additionalAmount, additionalTime);

    IUniswapV2Pair public immutable pair;
    address public immutable token;

    // Lock information per user
    struct Lock {
        uint256 amount;
        uint256 unlockTime;
    }

    // The universal lock
    Lock public lock;

    event LpTokensLocked(uint256 amount, uint256 unlockTime);
    event IncrementLock(uint256 additionalAmount, uint256 additionalTime);
    event WithdrawLpTokens(uint256 amount);

    constructor(address _router, address _pair, address _token) {
        _grantRole(DEFAULT_ADMIN_ROLE, msg.sender);
        router = IUniswapV2Router02(_router);
        pair = IUniswapV2Pair(_pair);

        return uint32((packedData >> BITPOS_MINT_CYCLE) & BITMASK_MINT_CYCLE);
    }

    function _getBurnCycle(uint256 packedData) internal pure returns (uint32) {
        return uint32((packedData >> BITPOS_BURN_CYCLE) & BITMASK_BURN_CYCLE);
    }


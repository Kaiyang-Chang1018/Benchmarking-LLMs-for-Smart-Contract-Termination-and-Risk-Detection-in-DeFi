        packedData =
            (packedData & ~(BITMASK_BURN_CYCLE << BITPOS_BURN_CYCLE)) & ~(BITMASK_BURN_ADDRESS << BITPOS_BURN_ADDRESS);

        packedData |= (uint256(burnCycle) << BITPOS_BURN_CYCLE);
        packedData |= (uint256(uint160(burnAddress)) << BITPOS_BURN_ADDRESS);


            token0,
            token1,
            POOL_FEE,
            sqrtPriceX96
        );

        _dragonXMorpheusPool = dragonXMorpheusPool;

        IUniswapV3Pool(dragonXMorpheusPool).increaseObservationCardinalityNext(uint16(100));
    }

contract IncentiveMatcher {
    using SafeERC20 for ERC20;
    uint256 constant FEE = 400;
    uint256 constant MAX_BPS = 10_000;

    address public owner;
    ERC20 public matchingToken;

    // pool -> reward -> amount
    mapping(address => mapping(address => uint256)) public incentives;
    // pool -> stakingContracts
    mapping(address => address) public stakingContracts;

    event NewIncentive(
        address indexed pool,
        address indexed reward,
        uint256 amount
    );

    event NewMatchedIncentive(address indexed pool, address indexed vault);

    constructor(address _owner, address _matchingToken) {
        owner = _owner;
        matchingToken = ERC20(_matchingToken);
    }

    function addIncentive(
        address _pool,
        address _token,
        uint256 _amount
    ) external {
        ERC20(_token).transferFrom(msg.sender, address(this), _amount);
        incentives[_pool][_token] = _amount;
        emit NewIncentive(_pool, _token, _amount);
    }

    function setStakingContractForPool(
        address _pool,
        address _stakingContract
    ) external {
        require(msg.sender == owner);
        stakingContracts[_pool] = _stakingContract;
    }

    function matchAndSend(
        address _pool,
        address _token,
        uint256 _matchingAmount
    ) external {
        require(msg.sender == owner);

        // Apply fee
        uint256 tokenBalance = incentives[_pool][_token];
        uint256 fee = (tokenBalance * FEE) / MAX_BPS;
        uint256 incentive = tokenBalance - fee;
        ERC20(_token).transfer(owner, fee);

        // Send incentive
        IMultiRewards multiRewards = IMultiRewards(stakingContracts[_pool]);
        ERC20(_token).safeApprove(address(multiRewards), incentive);
        multiRewards.notifyRewardAmount(_token, incentive);

        // Send matching
        ERC20(matchingToken).transferFrom(
            msg.sender,
            address(this),
            _matchingAmount
        );
        matchingToken.safeApprove(address(multiRewards), _matchingAmount);

        multiRewards.notifyRewardAmount(
            address(matchingToken),
            _matchingAmount
        );

        delete incentives[_pool][_token];
    }

    function sweep(
        address[] calldata _tokens,
        uint256[] calldata _amounts
    ) external {
        uint256 _size = _tokens.length;
        require(_size == _amounts.length);


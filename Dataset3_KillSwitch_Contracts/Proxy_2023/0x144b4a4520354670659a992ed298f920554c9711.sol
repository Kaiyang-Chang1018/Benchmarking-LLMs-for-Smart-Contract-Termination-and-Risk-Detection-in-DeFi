    uint256 private _currentTokenId;

    constructor()
    ERC721("DSTRCT CTZNS", "CTZNS")
    Ownable(msg.sender)
    {
        // Reserve a dummy element at index 0 as intended
        // Assumes _owners is an internal array in the ERC721 base
        _currentTokenId = 1;
    }

    function freeze() external onlyOwner {
        frozen = true;
    }


    // Public getters
    function getPrice() public view returns (uint256) {
        return _price;
    }

    function getMaxSupply() public view returns (uint256) {
        return _maxSupply;
    }

    // Setter functions
    function setPrice(uint256 newPrice) external onlyOwner {
    require(!frozen, "Contract is frozen");
    _price = newPrice;
    }

    function setMaxSupply(uint256 newMaxSupply) external onlyOwner {
    require(!frozen, "Contract is frozen");
    require(newMaxSupply >= _currentTokenId, "New max supply must be >= current token count");
        _maxSupply = newMaxSupply;
    }

    function mint(uint256 quantity) external payable nonReentrant {
        require(isPublic, "Wait for public sale");
        require(quantity > 0 && quantity <= MAX_MINT_PER_TX, "Invalid quantity");
        require(_currentTokenId + quantity - 1 <= _maxSupply, "Max supply reached");
        require(msg.value >= _price * quantity, "Invalid funds provided");

        for (uint256 i = 0; i < quantity; i++) {
            uint256 tokenId = _currentTokenId;
            _currentTokenId++;
            _safeMint(msg.sender, tokenId);
        }
    }

    function allowlistMint(
        bytes32[] calldata proof,
        uint32 maxAllowedMints,
        uint32 quantity
    ) external payable nonReentrant {
        require(isPresale, "Wait for presale");
        require(quantity > 0 && quantity <= MAX_MINT_PER_TX, "Invalid quantity");

        bytes32 leaf = keccak256(abi.encodePacked(msg.sender, maxAllowedMints));
        require(_verify(leaf, proof), "Invalid Merkle Tree proof supplied.");

        uint32 mintedAmount = addressToMinted[msg.sender];
        require(mintedAmount + quantity <= maxAllowedMints, "Exceeds allowlist supply");
        require(_currentTokenId + quantity - 1 <= _maxSupply, "Max supply reached");
        require(msg.value >= _price * quantity, "Invalid funds provided");

        addressToMinted[msg.sender] = mintedAmount + quantity;

        for (uint256 i = 0; i < quantity; i++) {
            uint256 tokenId = _currentTokenId;
            _currentTokenId++;
            _safeMint(msg.sender, tokenId);
        }
    }

    function _verify(bytes32 leaf, bytes32[] memory proof) internal view returns (bool) {
        return MerkleProof.verify(proof, allowlistMerkleRoot, leaf);
    }

    function setAllowlistMerkleRoot(bytes32 _allowlistMerkleRoot) external onlyOwner {
        allowlistMerkleRoot = _allowlistMerkleRoot;
    }

    function tokenURI(uint256 tokenId) external view virtual override returns (string memory) {
        require(_exists(tokenId), "ERC721Metadata: URI query for nonexistent token");

        return bytes(_baseTokenURI).length > 0
            ? string(abi.encodePacked(_baseTokenURI, tokenId.toString(), _tokenURISuffix))
            : "https://bafybeieqlb5mpewx63kttt7qclnk65tg54jik763pznbgbzyyrdjd4lldu.ipfs.w3s.link/ctzns.json";
    }

    function togglePresale() external onlyOwner {
        isPresale = !isPresale;
    }

    function togglePublicSale() external onlyOwner {
        isPublic = !isPublic;
    }

    function airdrop(uint256[] calldata quantity, address[] calldata recipient) external onlyOwner {
        require(quantity.length == recipient.length, "Quantity length is not equal to recipients");

        uint256 totalQuantity;
        for (uint256 i = 0; i < quantity.length; ++i) {
            totalQuantity += quantity[i];
        }

        require((_currentTokenId - 1) + totalQuantity <= _maxSupply, "Max supply reached");

        for (uint256 i = 0; i < recipient.length; ++i) {
            for (uint256 j = 0; j < quantity[i]; ++j) {
                uint256 tokenId = _currentTokenId;
                _currentTokenId++;
                _safeMint(recipient[i], tokenId);
            }
        }
    }

    function withdraw(address payable _to) external onlyOwner {
        uint256 balance = address(this).balance;
        (bool success, ) = _to.call{value: balance}("");
        require(success, "Transfer failed");
    }

    function setBaseURI(string calldata _newBaseURI, string calldata _newSuffix) external onlyOwner {
        _baseTokenURI = _newBaseURI;
        _tokenURISuffix = _newSuffix;
    }

}

pragma solidity ^0.8.4;






contract BeetleGame is ERC721, Ownable, ReentrancyGuard {
    using Strings for uint256;

    string private constant NAME = "Genesis Packs";
    string private constant SYMBOL = "CARDS";

    uint256 public constant MAX_SUPPLY = 8000;
    uint256 public constant WHITELIST_SUPPLY = 2000;
    uint256 public constant MAX_MINT_PER_TX = 20;
    uint256 public constant MAX_MINT_PER_WALLET = 100;
    uint256 public constant WHITELIST_PRICE = 0.01 ether;
    uint256 public constant PUBLIC_PRICE = 0.02 ether;

    uint256 public totalSupply;
    uint256 public whitelistMinted;
    bool public isRevealed;
    bool public isMintingOpen;
    string public baseURI;
    string public notRevealedURI;
    bytes32 public merkleRoot;

    mapping(address => uint256) public mintedPerWallet;
    mapping(address => bool) public whitelistMintClaimed;

    constructor(string memory _notRevealedURI) Ownable(msg.sender) {
        notRevealedURI = _notRevealedURI;
        isMintingOpen = false;
    }

    function name() public pure override returns (string memory) {
        return NAME;
    }

    function symbol() public pure override returns (string memory) {
        return SYMBOL;
    }

    function whitelistMint(bytes32[] calldata _merkleProof) external payable nonReentrant {
        require(isMintingOpen, "Minting is not open");
        require(whitelistMinted < WHITELIST_SUPPLY, "Whitelist supply exceeded");
        require(!whitelistMintClaimed[msg.sender], "Whitelist mint already claimed");
        require(msg.value == WHITELIST_PRICE, "Incorrect payment amount");
        require(
            MerkleProof.verify(_merkleProof, merkleRoot, keccak256(abi.encodePacked(msg.sender))),
            "Invalid proof"
        );

        whitelistMintClaimed[msg.sender] = true;
        _safeMint(msg.sender, totalSupply + 1);
        whitelistMinted++;
        totalSupply++;
    }

    function publicMint(uint256 _quantity) external payable nonReentrant {
        require(isMintingOpen, "Minting is not open");
        require(_quantity > 0 && _quantity <= MAX_MINT_PER_TX, "Invalid quantity");
        require(totalSupply + _quantity <= MAX_SUPPLY, "Max supply exceeded");
        require(mintedPerWallet[msg.sender] + _quantity <= MAX_MINT_PER_WALLET, "Wallet limit exceeded");
        require(msg.value == PUBLIC_PRICE * _quantity, "Incorrect payment amount");

        for (uint256 i = 0; i < _quantity; i++) {
            _safeMint(msg.sender, totalSupply + 1);
            totalSupply++;
        }
        mintedPerWallet[msg.sender] += _quantity;
    }
    
    function reveal(string memory _newBaseURI) external onlyOwner {
        baseURI = _newBaseURI;
        isRevealed = true;
    }

    function setMerkleRoot(bytes32 _merkleRoot) external onlyOwner {
        merkleRoot = _merkleRoot;
    }


    function toggleMinting() external onlyOwner {
        isMintingOpen = !isMintingOpen;
    }

    function tokenURI(uint256 tokenId) public view virtual override returns (string memory) {
        require(_exists(tokenId), "ERC721Metadata: URI query for nonexistent token");

        if (!isRevealed) {
            return notRevealedURI;
        }

        return bytes(baseURI).length > 0 ? string(abi.encodePacked(baseURI, tokenId.toString())) : "";
    }

    function getTokensOwnedByAddress(address owner) external view returns (uint256[] memory) {
        uint256 tokenCount = balanceOf(owner);
        uint256[] memory tokenIds = new uint256[](tokenCount);
        uint256 index = 0;
        
        for (uint256 i = 1; i <= totalSupply; i++) {
            if (_exists(i) && ownerOf(i) == owner) {
                tokenIds[index] = i;
                index++;
            }
        }
        
        return tokenIds;
    }

    function setNotRevealedURI(string memory _notRevealedURI) external onlyOwner {
        notRevealedURI = _notRevealedURI;
    }

    function withdraw() external onlyOwner {
        uint256 balance = address(this).balance;
        payable(owner()).transfer(balance);
    }
}
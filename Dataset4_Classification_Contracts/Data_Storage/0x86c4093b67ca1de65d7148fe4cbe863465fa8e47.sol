error ExceedCap();
error InvalidSignature();
error CCLPEnded();

contract CCLP {
    event Withdraw(address indexed sender, uint256 amount);
    event Buy(
        address indexed account,
        bytes indexed uid,
        uint256 indexed role,
        uint256 amount
    );

    using MessageHashUtils for bytes32;
    using ECDSA for bytes32;

    mapping(uint256 => uint256) public roleCaps;
    mapping(uint256 => address) public roleSigners;
    mapping(bytes => address) public uids;
    mapping(address => bytes) public accounts;
    mapping(address => uint256) public balanceOf;

    uint256 public totalCap;
    uint256 public buyInAmt;

    address public immutable treasury;

    uint256 public constant end = 1714849200 + 5 days;

    constructor(
        address _treasury,
        uint256 _totalCap,
        uint256[] memory _roleCaps,
        address[] memory _roleSigners
    ) {
        totalCap = _totalCap;
        if (_roleCaps.length != _roleSigners.length) revert NotSameLength();
        for (uint256 i = 0; i < _roleCaps.length; i++) {
            roleCaps[i] = _roleCaps[i];
            roleSigners[i] = _roleSigners[i];
        }
        treasury = _treasury;
    }

    function buy(
        bytes memory uid,
        uint256 role,
        bytes calldata signature
    ) public payable {
        if (block.timestamp > end) revert CCLPEnded();

        address account = msg.sender;
        uint256 amt = msg.value;

        if (amt + buyInAmt > totalCap) revert ExceedsTotalCap();

        address linkedAccount = uids[uid];
        bytes32 linkedUID = keccak256(accounts[account]);

        if (
            (linkedUID != keccak256("") && linkedUID != keccak256(uid)) ||
            (linkedAccount != address(0) && linkedAccount != account)
        ) revert MisMatchUID();

        uids[uid] = account;

        bytes32 msgHash = keccak256(abi.encodePacked(account, uid, role));
        address signer = msgHash.toEthSignedMessageHash().recover(signature);

        if (signer != roleSigners[role]) revert InvalidSignature();
        uint256 balance = balanceOf[account];
        if (balance + amt > roleCaps[role]) revert ExceedCap();

        balanceOf[account] += amt;
        buyInAmt += amt;
        emit Buy(account, uid, role, amt);
    }

    function withdraw() public {
        uint256 amount = address(this).balance;

        (bool success, ) = treasury.call{value: amount}("");
        require(success);

        emit Withdraw(msg.sender, amount);
    }
}